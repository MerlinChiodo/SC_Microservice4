    "properties": {
        "event_id" : {
            "type": "integer",
            "minimum": 0,
            "maximum": 10000
        },
        "event_name": {
            "type": "string"
        },
        "title": {
            "type": "string"
        },
        "text_short": {
            "type": "string"
        },
        "text_long": {
            "type": "string"
        },
        "service_name": {
            "type": "string"
        },
        "date": {
            "type": "string",
            "format": "date-time"
        },
        "picture_url": {
            "type": "string",
            "minLength": 1
        }
    },
    "required": [
        "event_id",
        "title",
        "text_short",
        "service_name",
        "date"
    ],
    "additionalProperties": false
}

